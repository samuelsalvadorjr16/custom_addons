                                            <t t-set="cpt_numbers_summary" t-value="loop[doc_loop][0].get_report_summary_refno(loop[doc_loop])"/>
                                            <tr>

                                                <t t-set="untax_amount" t-value="float(cpt_numbers_summary[1])"/>
                                                <t t-set="tax_amount_1" t-value="float(cpt_numbers_summary[2])"/>
                                                <t t-set="tax_amount_2" t-value="float(cpt_numbers_summary[3])"/>
                                                <t t-set="tax_amount" t-value="float(cpt_numbers_summary[4])"/>
                                                <t t-set="net_amount" t-value="float(cpt_numbers_summary[5])"/>
