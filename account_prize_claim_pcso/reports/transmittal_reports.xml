<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">


        <template id="assets_common" name="voucher printing assets" inherit_id="report.assets_common">
            <xpath expr="." position="inside">
                <link href="/account_prize_claim_pcso/static/src/less/voucher.less" rel="stylesheet"/>
            </xpath>
        </template>



        <record id="paperformat_transmittal" model="report.paperformat">
            <field name="name">Transmittal Report</field>
            <field name="default" eval="False" />
            <field name="format">custom</field>
            <field name="page_height">331</field>
            <field name="page_width">216</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">7</field>
            <field name="margin_bottom">7</field>
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">0</field>
            <field name="dpi">90</field>
        </record>

<template id="external_layout_footer_transmitter">
    <div class="footer">

        <!--
        <div class="text-left">
            Date Printed : <span t-esc="context_timestamp(datetime.datetime.now()).strftime('%m/%d/%Y')"/>
        </div>
    -->

        <div class="text-right" style="border-top: 1px solid black;">
            <ul class="list-inline">
                <li>Page:</li>
                <li><span class="page"/></li>
                <li>/</li>
                <li><span class="topage"/></li>
            </ul>
        </div>
    </div>      
</template>


<template id="external_layout_header_transmitter">
    <div class="header">
        <div class="text-right">
            <ul class="list-inline">
                <li>Page:</li>
                <li><span class="page"/></li>
                <li>/</li>
                <li><span class="topage"/></li>
            </ul>
        </div>
    </div>  
</template>


<template id="external_layout_transmitter">
    <t t-raw="0" />
    <t t-call="account_prize_claim_pcso.external_layout_footer_transmitter" />
</template>




        <template id="print_transmittal_report">
            <t t-set= "loop" t-value="{}" />
            <t t-foreach="docs" t-as="doc">
                <t t-set= "loop" t-value="doc.get_cpt_numbers(loop)" />
                        
            </t>    

            <t t-foreach="loop" t-as="doc_loop">
                <t t-call="report.html_container">
                    <t t-call="account_prize_claim_pcso.external_layout_transmitter"> 
                            <div class="page" id="voucher_print">
                                  <t t-set="date_max_min" t-value="loop[doc_loop][0].get_get_dates(loop)"/>
                                  <div class="text-center"><p>PHILIPPINE CHARITY SWEEPSTAKES OFFICE</p></div>
                                  <div class="text-center"><small>CHARITY ASSISTANCE DEPARTMENT</small></div>
                                  <div class="text-center"><small>SUMMARY OF INDIVIDUAL MEDICAL ASSISTANCE PROGRAM(IMAP) AVAILMENT</small></div>
                                  <div class="text-center"><small>CHARITY TRANSMITTAL DATE FROM <t t-esc="date_max_min[0]"  t-esc-options='{"widget": "date", "format": "MMMM dd, YYYY"}'/> TO <t t-esc="date_max_min[1]" t-esc-options='{"widget": "date", "format": "MMMM dd, YYYY"}'/> </small></div>
                                  <div class="text-center"><small>TRANSMITTAL NUMBER <u>&amp;emsp;<t t-esc="int(loop[doc_loop][0].transmittal_charity_number[4:len(loop[doc_loop][0].transmittal_charity_number)])" />&amp;emsp;</u></small></div>
                                  <t t-set= "start_num" t-value="1" />
                                  <t t-set= "is_for_start_num" t-value="1" />
